@model List<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>
<table class ="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
            <th>
                Created At
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="33%">
                    @obj.Name
                </td>
                <td width="33%">
                    @obj.DisplayOrder
                </td>
                <td width="33%">
                    @obj.CreatedDateTime
                </td>
            </tr>

        }
    </tbody>
</table>
